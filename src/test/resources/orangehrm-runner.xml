<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="orangehrm test">

    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>
    <parameter name="browserName" value="Chrome"></parameter>
    <parameter name="url" value="https://opensource-demo.orangehrmlive.com/"></parameter>
    <test name="orangehrm tests">
        <classes>
            <class name="com.orangehrm.test.TestLogIn"></class>
            <class name="com.orangehrm.test.TestLogOut"></class>
            <class name="com.orangehrm.test.TestAddInfoEverything"></class>
            <class name="com.orangehrm.test.TestManagerReviews"></class>
            <class name="com.orangehrm.test.TestMostLikePost"></class>
            <class name="com.orangehrm.test.TestPostComments"></class>
            <class name="com.orangehrm.test.TestPIM"></class>
            <class name="com.orangehrm.test.TestQualifications"></class>
            <class name="com.orangehrm.test.TestReportsOfQAEngineers"></class>
            <class name="com.orangehrm.test.TestTimeFunctionAll"></class>
            <class name="com.orangehrm.test.TestTimeSheets"></class>
            <class name="com.orangehrm.test.AdminPageTest"></class>
        </classes>
    </test>
</suite>